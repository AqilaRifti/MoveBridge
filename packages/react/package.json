{
    "name": "@movebridge/react",
    "version": "0.0.1",
    "description": "React hooks and components for Movement Network",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsup src/index.ts --format cjs,esm --dts --clean --external react",
        "dev": "tsup src/index.ts --format cjs,esm --dts --watch --external react",
        "clean": "rm -rf dist",
        "typecheck": "tsc --noEmit",
        "test": "vitest run",
        "test:watch": "vitest"
    },
    "dependencies": {
        "@movebridge/core": "workspace:*"
    },
    "devDependencies": {
        "@testing-library/react": "^14.1.0",
        "@types/react": "^18.2.42",
        "@types/react-dom": "^18.2.17",
        "jsdom": "^23.0.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "tsup": "^8.0.1",
        "typescript": "^5.3.2"
    },
    "peerDependencies": {
        "react": "^18.0.0",
        "react-dom": "^18.0.0"
    },
    "keywords": [
        "movement",
        "aptos",
        "react",
        "hooks",
        "wallet",
        "web3"
    ],
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/movebridge/movebridge-sdk",
        "directory": "packages/react"
    }
}